    "type": "library",
    "keywords": ["martha", "continuous integration", "github"],
    "homepage": "http://github.com/martha-ci",
    "license": "MIT",
    "authors": [
        {
            "name": "Kristopher Wilson",
            "email": "kristopherwilson@gmail.com",
            "homepage": "http://kristopherwilson.com",
            "role": "Developer"
        }
